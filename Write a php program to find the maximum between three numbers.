<?php
	
	//Write a php program to find the maximum between three numbers.

	echo "Enter the num1\tnum2\tnum3 \n";

	 $num1 = readline('Input the num1 : ');
	 $num2 = readline('Input the num2 : ');
	 $num3 = readline('Input the num3 : ');

	 if (($num1>$num2) && ($num1>$num3)) 
	 {
	 	echo "num1 is max : ". $num1;
	 }
	 if (($num2>$num1) && ($num2>$num3)) 
	 {
	 	
	 	echo "num2 is max : ". $num2;
	 }
	 if (($num3>$num1) && ($num3>$num2)) 
	 {
	 	
	 	echo "num3 is max : ". $num3;
	 }

	
?>
